{
  "name": "server",
  "private": true,
  "scripts": {
    "start": "run-p dev:*",
    "dev:server": "meteor run --settings settings.json --port 3033",
    "dev:ngrok": "ngrok http 3033  >/dev/null 2>&1",
    "dev:get-ngrok-url": "node --experimental-modules tests/scripts/ngrok.mjs",
    "logs": "mup logs --tail --follow",
    "dev:test": "jest --watch",
    "test": "jest",
    "generate-keys": "node --experimental-modules tests/scripts/generateKeyPairs.js"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/runtime": "^7.15.4",
    "@types/meteor": "^2.6.1",
    "@types/react-dom": "^17.0.14",
    "@types/web-push": "^3.3.2",
    "body-parser": "^1.19.1",
    "device-detector-js": "^3.0.1",
    "express": "^4.17.2",
    "google-auth-library": "^7.11.0",
    "google-libphonenumber": "^3.2.26",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.3.2",
    "jest": "^27.5.1",
    "meteor-node-stubs": "^1.1.0",
    "node-fetch": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.4",
    "react-linkify": "^1.0.0-alpha",
    "typescript": "^4.6.3",
    "web-push": "^3.4.5"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.tsx"
    }
  },
  "devDependencies": {
    "ngrok": "^4.3.1",
    "npm-run-all": "^4.1.5"
  }
}
